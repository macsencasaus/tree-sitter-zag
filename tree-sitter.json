  "grammars": [
    {
      "name": "zag",
      "camelcase": "Zag",
      "title": "Zag",
      "scope": "source.zag",
      "file-types": [
        "zag"
      ],
      "injection-regex": "^zag$",
      "class-name": "TreeSitterZag"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Zag Grammar",
    "authors": [
      {
        "name": "Macsen Casaus",
        "email": "macsencasaus@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/macsencasaus/tree-sitter-zag"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}